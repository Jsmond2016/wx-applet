/* pages/template/list-template.wxss */
.temContainer{
  display: flex;
  flex-direction: column;
}

.avatar_data image{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 10rpx;
}

.avatar_data text{
  font-size: 32rpx;
}

.company{
  margin-left: 10rpx;
  font-size: 36rpx;
  font-weight: bold;
  margin: 10rpx;
}

.contentImg{
  width: 100%;
  height: 460rpx;
}
.content{
  font-size: 32rpx;
  text-indent: 32rpx;
}

.collection_love image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.collection_love text{
  font-size: 28rpx;
  margin-right: 10rpx;
}